{
  "name": "@adobe/node-openwhisk-newrelic",
  "description": "Library for gathering metrics for Apache OpenWhisk actions and sending them to NewRelic",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "main": "index.js",
  "author": {
    "name": "Adobe Systems Inc."
  },
  "repository": "adobe/node-openwhisk-newrelic",
  "dependencies": {
    "cls-hooked": "^4.2.2",
    "debug": "^4.1.1",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@adobe/eslint-config-asset-compute": "^1.0.0",
    "@adobe/httptransfer": "^1.0.0",
    "axios": "^0.19.2",
    "clone": "^2.1.2",
    "fs-extra": "^8.1.0",
    "mocha": "^7.0.0",
    "mock-fs": "^4.12.0",
    "mock-http-server": "^1.4.2",
    "needle": "^2.4.1",
    "nock": "^12.0.3",
    "nyc": "^15.0.1",
    "pem": "^1.14.4",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8"
  },
  "keywords": [
    "scripts",
    "openwhisk",
    "new relic",
    "library",
    "metrics",
    "adobe"
  ],
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha test --recursive && npm run test1 && npm run test2 && npm run test3 && npm run test4 && npm run test-node-fetch",
    "test1": "rm -rf node-fetch; git clone https://github.com/node-fetch/node-fetch.git",
    "test2": "cp test/node-fetch-disable-flaky-tests.patch node-fetch/node-fetch-disable-flaky-tests.patch",
    "test3": "cd node-fetch; git checkout b44c29521d61e0badb0220ac4d59bcfeef986ea2; npm install; cd node-fetch; patch -p1 < node-fetch-disable-flaky-tests.patch",
    "test4": "cp util/node-fetch.setup.js node-fetch/node-fetch.setup.js",
    "test-node-fetch": "cd node-fetch && npm test -- --file node-fetch.setup.js",
    "posttest": "eslint ./",
    "beautify": "eslint ./ --fix"
  },
  "mocha": {
    "file": "test/logfile.setup.js"
  }
}
